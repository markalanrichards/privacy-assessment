{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "no-shadowed-variable": false,
    "prettier": true,
    "interface-name": [false, "always-prefix"],
    "object-literal-sort-keys": false
  }
}
